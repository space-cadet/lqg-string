	issn = {2367-3613, 1433-8351},
	journal = {Living Reviews in Relativity},
	keywords = {High Energy Physics - Theory},
	month = dec,
	number = {1},
	pages = {9},
	title = {Spinning Strings and Integrable Spin Chains in the {{AdS}}/{{CFT}} Correspondence},
	urldate = {2023-09-03},
