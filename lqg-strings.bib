	month = {jan},
	number = {8},
	pages = {086015},
	title = {{The Second Law of Quantum Complexity}},
	url = {https://link.aps.org/doi/10.1103/PhysRevD.97.086015 http://arxiv.org/abs/1701.01107 http://dx.doi.org/10.1103/PhysRevD.97.086015},
