	issn = {13552198},
	journal = {Studies in History and Philosophy of Science Part B: Studies in History and Philosophy of Modern Physics},
	keywords = {High Energy Physics - Theory,Physics - History and Philosophy of Physics},
	month = feb,
	pages = {82--111},
	primaryclass = {hep-th, physics:physics},
	title = {Conceptual {{Analysis}} of {{Black Hole Entropy}} in {{String Theory}}},
