@article{Jacobson1995Thermodynamics,
	abstract = {The Einstein equation is derived from the proportionality of entropy and horizon area together with the fundamental relation \textbackslash\${$\delta$}Q=TdS\textbackslash\$ connecting heat, entropy, and temperature. The key idea is to demand that this relation hold for all the local Rindler causal horizons through each spacetime point, with \textbackslash\${$\delta$}Q\textbackslash\$ and \textbackslash\$T\textbackslash\$ interpreted as the energy flux and Unruh temperature seen by an accelerated observer just inside the horizon. This requires that gravitational lensing by matter energy distorts the causal structure of spacetime in just such a way that the Einstein equation holds. Viewed in this way, the Einstein equation is an equation of state. This perspective suggests that it may be no more appropriate to canonically quantize the Einstein equation than it would be to quantize the wave equation for sound in air.},
	author = {Jacobson, Ted},
	date-added = {2023-10-30 23:57:47 +0530},
	date-modified = {2023-10-30 23:57:48 +0530},
	file = {/Volumes/Data/owncloud/root/research/zotero_pdfs/Jacobson_Thermodynamics of spacetime -The Einstein equation of state_1995.pdf},
	journal = {Physical Review Letters},
	keywords = {black\_holes,black\textbackslash\_holes,general\_relativity,general\textbackslash\_relativity,horizons,second\_law,second\textbackslash\_law,statistical\_mechanics,statistical\textbackslash\_mechanics,thermodynamics},
	month = jun,
