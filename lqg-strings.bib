	issn = {10950761},
	journal = {Advances in Theoretical and Mathematical Physics},
	keywords = {antidesitter,cft,holography,large\_n\_theories,m\_theory,maldacena\_conjecture,susy},
	month = jan,
	number = {2},
	pages = {231--252},
	title = {The Large {{N Limit}} of Superconformal Field Theories and Supergravity},
	volume = {2},
