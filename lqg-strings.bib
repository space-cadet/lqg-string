	eprint = {hep-th/9501090},
	file = {/Volumes/Data/owncloud/root/research/zotero_pdfs/Das_Geometric Entropy of Nonrelativistic Fermions and Two Dimensional Strings_1995.pdf;/Volumes/Data/owncloud/root/research/zotero/storage/VKPMI6MX/9501090.html},
	issn = {0556-2821},
	journal = {Physical Review D},
	keywords = {High Energy Physics - Theory},
	month = jun,
	number = {12},
	pages = {6901--6908},
	title = {Geometric {{Entropy}} of {{Nonrelativistic Fermions}} and {{Two Dimensional Strings}}},
	urldate = {2023-11-17},
